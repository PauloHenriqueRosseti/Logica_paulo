programa {
  funcao inicio() {
    real um, do, tr, dm

    escreva ("coloque um numero: ")
    leia (um)

    escreva ("coloque um numero: ")
    leia (do)

    escreva ("coloque um numero: ")
    leia (tr)

    dm = um + do
    
